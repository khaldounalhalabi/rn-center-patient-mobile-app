{
  "expo": {
    "name": "Reslan Alnaal Center Patient App",
    "slug": "reslan-alnaal-center-patient-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "reslanalnaalcenterpatientapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "package": "reslan.alnaal.center.patient.app",
      "icon": "./assets/images/app-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-localization",
        {
          "supportedLocales": {
            "ios": ["en", "ar"],
            "android": ["en", "ar"]
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-notifications",
      "expo-media-library",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Kodchasan/Kodchasan-Regular.ttf",
            "./assets/fonts/Cairo/Cairo-Regular.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "kodchasan",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Kodchasan/Kodchasan-Bold.ttf",
                    "weight": 700
                  },
                  {
                    "path": "./assets/fonts/Kodchasan/Kodchasan-Light.ttf",
                    "weight": 300
                  },
                  {
                    "path": "./assets/fonts/Kodchasan/Kodchasan-ExtraLight.ttf",
                    "weight": 200
                  },
                  {
                    "path": "./assets/fonts/Kodchasan/Kodchasan-Medium.ttf",
                    "weight": 500
                  },
                  {
                    "path": "./assets/fonts/Kodchasan/Kodchasan-SemiBold.ttf",
                    "weight": 600
                  }
                ]
              },
              {
                "fontFamily": "cairo",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Cairo/Cairo-Bold.ttf",
                    "weight": 700
                  },
                  {
                    "path": "./assets/fonts/Cairo/Cairo-Light.ttf",
                    "weight": 300
                  },
                  {
                    "path": "./assets/fonts/Cairo/Cairo-ExtraLight.ttf",
                    "weight": 200
                  },
                  {
                    "path": "./assets/fonts/Cairo/Cairo-Medium.ttf",
                    "weight": 500
                  },
                  {
                    "path": "./assets/fonts/Cairo/Cairo-SemiBold.ttf",
                    "weight": 600
                  },
                  {
                    "path": "./assets/fonts/Cairo/Cairo-ExtraBold.ttf",
                    "weight": 800
                  },
                  {
                    "path": "./assets/fonts/Cairo/Cairo-Black.ttf",
                    "weight": 400
                  }
                ]
              }
            ]
          }
        }
      ],
      [
        "expo-calendar",
        {
          "calendarPermission": "The app needs to access your calendar."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "supportsRTL": true
    },
    "owner": "khaldounalhalabi"
  }
}
